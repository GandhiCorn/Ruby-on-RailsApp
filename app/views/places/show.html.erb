<p id="notice"><%= notice %></p>

<h2><%= @place.name %></h2>

<p>
  <%= @place.street %> <br>
  <%= @place.zip %> <%= @place.city %>
</p>

<iframe width="425" height="350" frameborder="0" scrolling="no" marginheight="0" 
        marginwidth="0" src="https://maps.google.com/maps?q=<%= @place.street + @place.city %>&amp;output=embed"></iframe>
